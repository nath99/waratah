  "version": "2.0.0",
  "repository": {
    "type": "git",
    "url": "git@gitlab.symbiote.com.au:dpc/silverstripe-nsw-design-system.git"
  },
  "description": "Frontend integration of the NSW Design System for Silverstripe websites. This package.json adds extra libraries beyond those provided by nsw-design-system",
  "main": "index.js",
  "scripts": {
    "setup": "npm install",
    "start": "node_modules/.bin/gulp",
    "build": "node_modules/.bin/gulp build",
    "patch": "npx patch-package --patch-dir ../../../../../../../mysite/frontend/patches",
    "rebuild-node-sass": "npm rebuild node-sass",
    "buildall": "npm install --prefer-offline --silent --no-audit && npm run-script patch && npm run-script build",
    "audit": "npm audit"
  },
  "author": "NSWDPC Digital",
  "contributors": [
    {
      "name": "Symbiote"
    }
  ],
  "dependencies": {
    "flatpickr": "^4.6",
    "micromodal": "^0.4.2",
    "nsw-design-system": "^2.13.0",
    "slim-select": "^1.27",
    "sticky-js": "^1.3"
  },
  "devDependencies": {
    "@babel/core": "^7.10",
    "@babel/preset-env": "^7.10",
    "@babel/register": "^7.10",
    "core-js": "^3.9",
    "cssnano": "^4.1.10",
    "del": "^5.1.0",
    "gulp": "^4.0.2",
    "gulp-babel": "^8",
    "gulp-better-rollup": "^4.0.1",
    "gulp-concat": "^2.6.1",
    "gulp-debug": "^4.0.0",
    "gulp-filter": "^6.0.0",
    "gulp-postcss": "^8.0.0",
    "gulp-rename": "^2.0.0",
    "gulp-sass": "^4.0.2",
    "gulp-sourcemaps": "^2.6.5",
    "gulp-svg-sprite": "^1.5.0",
    "gulp-uglify-es": "^2",
    "minimist": "^1.2.5",
    "mkdirp": "^1.0.4",
    "patch-package": "^6.4.7",
    "rollup": "^1.4",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "yargs": "^16.2.0"
  }
}
