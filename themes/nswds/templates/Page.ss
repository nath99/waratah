<!doctype html>
<html lang="en" class="no-js">
<head>
    <% base_tag %>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <title>$Title</title>
    <meta name="robots" content="noindex" />
    <meta name="description" content="">
    <meta name="keywords" content="">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <link rel="icon" href=".../../favicon.ico">
    <link href="https://fonts.googleapis.com/css?family=Montserrat:400,600&display=swap" rel="stylesheet">
</head>

<body >

<div class="nsw-masthead">
  <div class="nsw-container">
    <p>A NSW Government website</p>
  </div>
</div>
<header class="nsw-header nsw-header--simple">
  <div class="nsw-header__container">
    <div class="nsw-header__wrapper">
      <div class="nsw-header__center">
        <a href="$BaseHref" class="nsw-header__logo-link">
          <svg class="nsw-icon" focusable="false" aria-hidden="true" style="width:12rem;height:4rem;">
            <use xlink:href="#dpc-logo-colour"></use>
          </svg>
          <span class="sr-only">DPC</span>
        </a>
      </div>
    </div>
</header>

<main>
  <div class="nsw-section--white">
    <div class="nsw-container">
      <div class="nsw-page-layout">
        <div class="nsw-page-layout__main">
            $Layout
        </div>
      </div>
    </div>
  </div>
</main>

<% include SVGSprite %>


</body>
</html>
